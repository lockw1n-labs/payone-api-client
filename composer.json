{
    "name": "lockw1n-labs/payone-api-client",
    "type": "library",
    "description": "PSP Payone API Client.",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "symfony/http-client": "^v7.0.0"
    },
    "require-dev": {
        "codeception/module-asserts": "*",
        "phpstan/phpstan": "*"
    },
    "autoload": {
        "psr-4": {
            "Lockw1nLabs\\": "src/Lockw1nLabs/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "stan": "vendor/bin/phpstan analyse src/",
        "test": "vendor/bin/codecept run --quiet --fail-fast --no-exit"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 600,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
